<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#ffffff">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="181dp"
        android:layout_height="238dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView4_24_7"
            android:layout_width="120dp"
            android:layout_height="93.06dp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4_24_7"
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="7dp"
            android:lines="2"
            android:maxLines="1"
            android:text="Blue round teal \n       tube light"
            android:textColor="#424242"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView4_24_7"
            app:layout_constraintStart_toStartOf="@+id/imageView4_24_7"
            app:layout_constraintTop_toBottomOf="@+id/imageView4_24_7" />

        <TextView
            android:id="@+id/desc"
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:lines="3"
            android:text="This light is in white and  blue both color and adjustable brigntness...... "
            android:textColor="#706C6C"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView4_24_7"
            app:layout_constraintStart_toStartOf="@+id/textView4_24_7"
            app:layout_constraintTop_toBottomOf="@+id/textView4_24_7" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="9dp"
            android:layout_height="12dp"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/vector2" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="9dp"
            android:layout_height="12dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            app:srcCompat="@drawable/vector" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>